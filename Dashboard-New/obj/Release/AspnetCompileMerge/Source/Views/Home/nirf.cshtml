<div class="dclist" id='scrolly'>
    <table border="1" id="customers">
        <tr style="color:#024b72">
            <th>Receipt</th>
            <th>Date</th>
            <th>Months</th>
            <th>CPRNO</th>           
            <th>CONS_TYPE</th>
            <th>DEPARTMENT CODE</th>
            <th>COORDINATOR NAME</th>
            <th>AGENCY CODE</th>
            <th>AGENCY</th>
            <th>DESCRIPTION</th>
            <th>RT</th>
        </tr>
        @foreach (var rec in Model.nirf)
        {
            <tr>
                <td>@rec.Receipt</td>  
                <td>@rec.Date</td>   
                <td>@rec.Months</td>       
                <td>@rec.CPRNO</td>
                <td>@rec.CONS_TYPE</td>
                <td>@rec.DEPT_CODE</td>
                <td>@rec.COOR_NAME1</td>
                <td>@rec.AGENCY_CODE</td>
                <td>@rec.AGENCY</td>
                <td>@rec.DESCRIPTION</td>
                <td>@rec.RT</td>
            </tr>
        }
    </table>
